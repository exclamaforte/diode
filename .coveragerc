[run]
source = diode
branch = True
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */.*
    setup.py
    build_packages.py
    */migrations/*
    */venv/*
    */virtualenv/*
    */site-packages/*
    */dist-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

ignore_errors = True
show_missing = True
precision = 2
skip_covered = False

[html]
directory = htmlcov

[xml]
output = coverage.xml
